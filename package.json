{
  "name": "attune-reviewer",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "npx nx serve attune-reviewer --configuration=development --port 4300",
    "build": "nx run attune-reviewer:build:production",
    "watch": "ng build --watch --configuration development",
    "test": "nx run attune-reviewer:test",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "husky install",
    "doc:local": "compodoc -p ./tsconfig.compodoc.json -s -o -w",
